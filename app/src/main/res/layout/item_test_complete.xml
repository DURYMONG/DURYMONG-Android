<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/iv_test_complete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="139dp"
        android:contentDescription="완료하기 네모"
        android:src="@drawable/img_test_complete_rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_test_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="47dp"
        android:contentDescription="연필 아이콘"
        android:src="@drawable/ic_test_complete"
        app:layout_constraintBottom_toBottomOf="@id/iv_test_complete"
        app:layout_constraintEnd_toEndOf="@id/iv_test_complete"
        app:layout_constraintStart_toStartOf="@id/iv_test_complete"
        app:layout_constraintTop_toTopOf="@id/iv_test_complete" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/suite_medium"
        android:text="완료하기"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@id/iv_test_icon"
        app:layout_constraintStart_toEndOf="@id/iv_test_icon"
        app:layout_constraintTop_toTopOf="@id/iv_test_icon" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="133dp"
        app:layout_constraintTop_toBottomOf="@id/iv_test_complete" />

</androidx.constraintlayout.widget.ConstraintLayout>