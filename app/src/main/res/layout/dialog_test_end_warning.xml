<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="342dp"
    android:layout_height="190dp">

    <TextView
        android:id="@+id/tv_warning_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/suite_semi_bold"
        android:text="정말로 검사를 끝내시겠어요?"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_warning_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/suite_medium"
        android:text="지금까지의 답변이 모두 삭제돼요!"
        android:textColor="@color/gray3"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_warning_title" />

    <ImageView
        android:id="@+id/iv_waring_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="38dp"
        android:contentDescription="돌아가기 버튼"
        android:src="@drawable/img_result_dialog_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_warning_content" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/suite_medium"
        android:text="돌아가기"
        android:textColor="@color/main_purple"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@id/iv_waring_back"
        app:layout_constraintEnd_toEndOf="@id/iv_waring_back"
        app:layout_constraintStart_toStartOf="@id/iv_waring_back"
        app:layout_constraintTop_toTopOf="@id/iv_waring_back" />

    <ImageView
        android:id="@+id/iv_waring_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:contentDescription="끝내기 버튼"
        android:src="@drawable/img_result_dialog_ok"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/iv_waring_back" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/suite_medium"
        android:text="끝내기"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@id/iv_waring_end"
        app:layout_constraintEnd_toEndOf="@id/iv_waring_end"
        app:layout_constraintStart_toStartOf="@id/iv_waring_end"
        app:layout_constraintTop_toTopOf="@id/iv_waring_end" />


</androidx.constraintlayout.widget.ConstraintLayout>